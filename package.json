{
  "name": "muleify",
  "version": "2.0.2",
  "description": "Website bundler. Static site generator.",
  "main": "index.js",
  "bin": {
    "muleify": "bin/muleify.js"
  },
  "scripts": {
    "serve": "node bin/muleify.js serve example/src example/dist",
    "pack": "node bin/muleify.js pack example/src example/dist",
	"encamp": "node bin/muleify.js encamp example/sitemap.json example",
	"map": "node bin/muleify.js map example/sitemap.json example",
    "component": "node bin/muleify.js component ./example/component.html",
    "help": "node bin/muleify.js -h "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderElias/muleify.git"
  },
  "keywords": [
    "build",
    "tool",
    "web",
    "components",
    "html",
    "css",
    "js",
    "cli",
    "gulp",
    "webpack",
    "webpacker",
    "webpacks",
    "webbundle",
    "webbundler",
    "webbundles",
    "web-pack",
    "web-packer",
    "web-packs",
    "web-bundle",
    "web-bundler",
    "web-bundles",
    "static",
    "site",
    "generator",
    "static-site",
    "static-site-generator",
    "muleify",
    "mule"
  ],
  "author": "Alexander Elias",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/AlexanderElias/muleify/issues"
  },
  "homepage": "https://github.com/AlexanderElias/muleify#readme",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "chalk": "^1.1.3",
    "clean-css": "^3.4.20",
    "commander": "^2.9.0",
    "css": "^2.2.1",
    "fsep": "^1.9.7",
    "hapi": "^14.2.0",
    "inert": "^4.0.2",
    "less": "^2.7.1",
    "node-sass": "^3.11.1",
    "node-watch": "^0.4.0",
    "porty": "^1.0.2",
    "rollup": "^0.34.13",
    "uglify-js": "^2.7.4"
  }
}
