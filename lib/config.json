{

	"babelOptions": {
		"compact": true,
		"minified": true,
		"comments": false,
		"presets": ["es2015"]
	},

	"ignoreables": [
		"node_modules",
		"jspm_modules"
	],

	"commentRegExpString": "((<!--\\s{)|(\/\\*\\s{))(.*?)((}\\s*\/)|(}\\s-->))",

	"startDataRegExpString": "(<!--\\s{)|(\/\\*\\s{)",
	"endDataRegExpString": "(}\\s-->)|(}\\s\\*\/)",

	"variableRegExpString": "((<!--\\s{)|(\/\\*\\s{))(\\s?)(\"|\\')(variable|var)(\"|\\')(:)(\\s?)(\"|\\')(.*?)(\"|\\')(\\s?)((}\\s*\/)|(}\\s-->))",
	"partialRegExpString": "((<!--\\s{)|(\/\\*\\s{))(\\s?)(\"|\\')(partial|par)(\"|\\')(:)(\\s?)(\"|\\')(.*?)(\"|\\')(\\s?)((}\\s*\/)|(}\\s-->))"
}
